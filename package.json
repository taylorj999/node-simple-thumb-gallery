{
  "name": "node-simple-thumb-gallery",
  "version": "0.0.1",
  "description": "node-simple-thumb-gallery",
  "scripts": {
    "setup-css": "copyfiles -f \"node_modules/bootstrap/dist/css/bootstrap.min.css\" public/css/",
    "setup-js": "copyfiles -f \"node_modules/bootstrap/dist/js/bootstrap.min.js\" \"node_modules/jquery/dist/jquery.min.js\" public/js/",
    "setup": "npm run setup-css && npm run setup-js",
    "start": "nodemon app.js"
  },
