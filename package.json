  "readmeFilename": "README.md",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "express": "4.17.1",
    "jquery": "^3.4.1",
    "mongodb": "^3.3.3",
    "popper.js": "^1.16.0",
    "validator": "11.0.0"
  }
}
